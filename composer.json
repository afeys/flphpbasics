    "keywords": [
        "array", "string", "number", "util", "helper", "manipulation", "encryption", 
        "multibyte", "chainable"
    ],
    "homepage": "https://github.com/afeys/flphpbasics",
    "license": "MIT",
    "authors": [
        {
            "name": "Andy Feys",
            "email": "gitprojects@feys.be",
            "homepage": "http://www.feys.be"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "support": {
        "issues": "https://github.com/afeys/flphpbasics/issues",
        "source": "https://github.com/afeys/flphpbasics/"
    },
    "autoload": {
        "psr-4": { "flphpbasics\\": "src/" },
